{
  "name": "mina-p2p-wasm-demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "parcel web/index.html --open",
    "wasm-pack": "wasm-pack build -t nodejs --out-name wasm",
    "transform": "node transform.js",
    "build": "yarn clean && parcel build web/index.html --no-source-maps",
    "build:full": "yarn wasm-pack && yarn transform && yarn build",
    "clean":"rm -r dist || echo 0"
  },
  "dependencies": {
    "events": "^3.3.0",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-hot-reload-api": "^2.3.4",
    "vue-property-decorator": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/events": "^3.0.0",
    "@vue/component-compiler-utils": "^3.3.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-wasm-pack": "^6.0.1",
    "sass": "^1.43.4",
    "typescript": "^4.5.2",
    "vue-template-compiler": "^2.6.14"
  }
}